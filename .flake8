[flake8]
max-line-length = 88
ignore = 
    E501,  # line too long
    W293,  # blank line contains whitespace
    W291,  # trailing whitespace
    E301,  # expected 1 blank line, found 0
    W292   # no newline at end of file
exclude = 
    .git,
    __pycache__,
    *.pyc,
    migrations,
    .venv,
    env,
    .env
per-file-ignores =
    __init__.py:F401
